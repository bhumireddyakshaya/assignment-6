import java.util.*;
 class Commonelements
{
    public static void main(String arg[]) 
{
        Integer[] array1 = {1, 2, 3, 4};
        Integer[] array2 = {2, 4, 5, 6, 7};
        Set<Integer> set1 = new Set(Arrays.asList(array1));
        Set<Integer> set2 = new Set(Arrays.asList(array2));
        set1.(set2);
        System.out.println("Common elements: " + set1);
    }
}
